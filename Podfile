# Uncomment this line to define a global platform for your project
platform :ios, '9.0'
inhibit_all_warnings!
use_frameworks!

target 'testEyeVersion' do
# Pods for proto-one
pod 'Alamofire', '~> 3.0' # Networking lib
pod 'AlamofireImage', '~> 2.0' # AlamofireImage is an image component library for Alamofire
pod 'AlamofireNetworkActivityIndicator', '~> 1.0' # Controls the visibility of the network activity indicator on iOS using Alamofire.
pod 'pop', '~> 1.0' # Facebook animation framework
pod 'Fabric' # Beta testing and crash reporting by Twitter
pod 'Crashlytics' # Crash reporting
pod 'SwiftyUserDefaults' # NSUserDefaults wrapper
pod 'KeychainAccess' # Keychain wrapper
pod 'XCGLogger'
pod 'SVProgressHUD'
pod 'ObjectMapper', '~> 1.3'
end

target 'testEyeVersionTests' do
pod 'Quick'
pod 'Nimble'
end
